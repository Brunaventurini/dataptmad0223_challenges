    "SELECT * FROM events;\n",
    "SELECT CAST(DATE_TRUNC('day', created_at) AS DATE) AS day, description, COUNT(*) AS count\n",
    "FROM events\n",
    "WHERE name = 'trained'\n",
    "GROUP BY day, description\n",
    "ORDER BY day;"
